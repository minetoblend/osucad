{
  "name": "osucad",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "concurrently \"npm run dev:server\" \"npm run dev:client\" --raw",
    "build": "concurrently \"npm run build:server\" \"npm run build:client\"",
    "dev:client": "vue-cli-service serve",
    "build:client": "vue-cli-service build",
    "dev:server": "nodemon --config ./nodemon.config.json",
    "build:server": "rimraf dist/server && tsc -p tsconfig.server.json"
  },
  "dependencies": {
    "axios": "^0.23.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^5.1.3",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "express-session": "^1.17.2",
    "express-ws": "^5.0.2",
    "mongoose": "^6.0.12",
    "p5": "^1.4.0",
    "passport": "^0.5.0",
    "passport-jwt": "^4.0.0",
    "passport-osu": "^1.0.8",
    "register-service-worker": "^1.7.1",
    "soundbank-pitch-shift": "^1.0.3",
    "uuid": "^8.3.2",
    "vue": "^3.0.0",
    "vue-axios": "^3.3.7",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express-session": "^1.17.4",
    "@types/express-ws": "^3.0.1",
    "@types/jest": "^24.0.19",
    "@types/p5": "^1.3.1",
    "@types/uuid": "^8.3.1",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "concurrently": "^6.3.0",
    "node-sass": "^4.12.0",
    "nodemon": "^2.0.14",
    "sass": "^1.43.3",
    "sass-loader": "^8.0.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "~4.1.5",
    "vue-jest": "^5.0.0-0"
  }
}
